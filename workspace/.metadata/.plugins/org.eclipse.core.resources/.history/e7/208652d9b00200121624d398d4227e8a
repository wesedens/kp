package com.wedens.codes.cb0;

import java.io.File;
import java.io.IOException;

public class CodeBreaker0 {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		
		if (args.length < 1){
			System.printf("usage: cb0 <filename>\n");
		}
		String filename = args[0];
		
		File f = new File(filename);
		if(f.exists()) {
			/* do something */ 
		}
		else{
			try {
				System.out.printf("File: %s does not exist\n", f.getCanonicalPath());
			} catch (IOException e) {
				// TODO Auto-generated catch block
				e.printStackTrace();
			}
		}
	}

}
